{
  "name": "peh",
  "private": true,
  "version": "2.4.0",
  "description": "Puru eye hospital",
  "scripts": {
    "build": "stencil build --prod --prerender --next && terser www/sw.js --compress -o www/sw.js",
    "start": "stencil build --dev --watch --serve --next --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "deploy": "firebase deploy",
    "build:deploy": "npm run build && npm run deploy"
  },
  "dependencies": {
    "@ionic/core": "5.0.5",
    "@stencil/core": "^1.9.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/sass": "^1.1.1",
    "imagemin": "^7.0.0",
    "imagemin-webp": "^5.1.0",
    "terser": "^4.6.3",
    "workbox-build": "4.3.1"
  }
}
